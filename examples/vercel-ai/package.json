{
  "name": "mcpd-vercel-ai-example",
  "version": "1.0.0",
  "description": "Example demonstrating mcpd SDK integration with Vercel AI SDK",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "lint": "npm run format:check && eslint . && npm run typecheck",
    "lint:fix": "npm run format && eslint . --fix && npm run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.69",
    "@mozilla-ai/mcpd": "file:../..",
    "ai": "^5.0.97"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.46.0"
  },
  "packageManager": "npm@11.6.2",
  "engines": {
    "node": ">=22.10.0"
  },
  "keywords": [
    "mcp",
    "mcpd",
    "vercel-ai",
    "ai",
    "tools",
    "example"
  ],
  "author": "Mozilla AI",
  "license": "Apache-2.0"
}
